<section class="container">
  <h3 class="gallery-name">Tutor Details</h3>
  <section class="flex-row justify-center">
    {{#each tutors}}
    <section class="card">
      <a href="/tutor/{{this.id}}">
        <header>{{this.name}}</header>
        <div class="card-body">
          <p>Email: {{this.email}}</p>
        </div>
      </a>
    </section>
    {{/each}}
  </section>
</section>
